{
  "name": "killrvideo-web",
  "version": "1.0.0",
  "description": "Web UI for the KillrVideo application demo.",
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "npm-run-all --parallel \"clean:*\"",
    "clean:client": "rimraf dist/client",
    "clean:server": "rimraf dist/server",
    "build": "npm-run-all --parallel build:client \"build:server:*\"",
    "build:client": "webpack --progress",
    "build:server": "npm-run-all --parallel \"build:server:*\"",
    "build:server:js": "babel src/server --out-dir dist/server --source-maps",
    "build:server:resources": "cpx src/server/resources/** dist/server/resources --verbose",
    "build:server:protos": "cpx lib/killrvideo-service-protos/src/** dist/server/resources/protos --verbose",
    "build:server:googleprotos": "cpx lib/google/** dist/server/resources/protos/google --verbose",
    "watch": "npm-run-all clean --parallel \"build:client -- --watch\" \"build:server:* -- --watch\"",
    "postversion": "npm-run-all push:commits push:tags",
    "push:commits": "git push",
    "push:tags": "git push --tags",
    "release:build": "cross-env NODE_ENV=production npm run build",
    "release": "npm-run-all clean release:build",
    "start": "node dist/server/index.js",
    "analyze": "webpack --json | webpack-bundle-size-analyzer > webpack-bundle-size-analyzer.log"
  },
  "author": "Luke Tillman <luke@luketillman.com> (http://www.luketillman.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeTillman/killrvideo-web.git"
  },
  "files": [
    "*.md",
    "dist"
  ],
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cpx": "^1.3.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "morgan": "^1.7.0",
    "npm-run-all": "^1.8.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-bundle-size-analyzer": "^2.0.1",
    "webpack-livereload-plugin": "^0.8.1"
  },
  "dependencies": {
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "bootswatch": "^3.3.6",
    "cassandra-driver": "^3.0.2",
    "cassandra-store": "^3.0.0",
    "classnames": "^2.2.3",
    "config": "^1.20.1",
    "d": "^1.0.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "falcor": "^0.1.16",
    "falcor-express": "^0.1.2",
    "falcor-http-datasource": "^0.1.3",
    "falcor-json-graph": "^1.1.7",
    "falcor-router": "^0.4.0",
    "font-awesome": "^4.6.1",
    "gemini-scrollbar": "^1.2.9",
    "grpc": "^0.14.1",
    "js-yaml": "^3.6.0",
    "jsuri": "^1.3.1",
    "load-script": "^1.0.0",
    "lodash": "^4.3.0",
    "md5": "^2.0.0",
    "memoizee": "^0.3.10",
    "moment": "^2.10.6",
    "passport": "^0.3.2",
    "ramda": "^0.21.0",
    "react": "^0.14.2",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.2",
    "react-dropzone": "^3.3.0",
    "react-gemini-scrollbar": "^2.0.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.4",
