{
  "name": "killrvideo-web",
  "version": "1.1.1",
  "description": "Web UI for the KillrVideo application demo.",
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "npm-run-all --parallel \"clean:*\"",
    "clean:client": "rimraf dist/client",
    "clean:server": "rimraf dist/server",
    "build": "npm-run-all --parallel build:client \"build:server:*\"",
    "build:client": "webpack --progress",
    "build:server": "npm-run-all --parallel \"build:server:*\"",
    "build:server:js": "babel src/server --out-dir dist/server --source-maps",
    "build:server:resources": "cpx \"src/server/resources/**\" dist/server/resources --verbose",
    "build:server:protos": "cpx \"lib/killrvideo-service-protos/src/**\" dist/server/resources/protos --verbose",
    "build:server:googleprotos": "cpx \"lib/google/**\" dist/server/resources/protos/google --verbose",
    "watch": "npm-run-all clean --parallel \"build:client -- --watch\" \"build:server:* -- --watch\"",
    "postversion": "git push --follow-tags",
    "release:build": "cross-env NODE_ENV=production npm run build",
    "release": "npm-run-all clean release:build",
    "start": "node dist/server/index.js",
    "analyze": "webpack --json | webpack-bundle-size-analyzer > webpack-bundle-size-analyzer.log"
  },
  "author": "Luke Tillman <luke@luketillman.com> (http://www.luketillman.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeTillman/killrvideo-web.git"
  },
  "homepage": "https://killrvideo.github.io",
  "files": [
    "*.md",
    "dist"
  ],
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cpx": "^1.3.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "morgan": "^1.7.0",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-bundle-size-analyzer": "^2.0.2",
    "webpack-livereload-plugin": "^0.8.1"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cassandra-driver": "^3.1.1",
    "cassandra-store": "^3.0.0",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "falcor-express": "^0.1.2",
    "falcor-json-graph": "^1.1.7",
    "falcor-router": "^0.4.0",
    "grpc": "^1.0.0-pre1",
    "js-yaml": "^3.6.1",
    "killrvideo-nodejs-common": "https://github.com/KillrVideo/killrvideo-nodejs-common/releases/download/v2.1.1/killrvideo-nodejs-common-2.1.1.tgz",
    "memoizee": "^0.3.10",
    "passport": "^0.3.2",
    "ramda": "^0.21.0",
    "socket.io": "^1.4.8",
    "uuid": "^2.0.2"
  }
}
