{
  "name": "killrvideo-web-server",
  "version": "0.0.0",
  "description": "A mocked/sample web server implementation for testing the KillrVideo Web UI (killrvideo-web)",
  "main": "out/index.js",
  "scripts": {
    "clean": "rimraf out",
    "build:js": "babel src --out-dir out --source-maps",
    "build:resources": "cpx src/resources/** out/resources --verbose",
    "build:client": "cpx node_modules/killrvideo-web/dist/** out/resources/static --verbose",
    "build:protos": "cpx lib/killrvideo-service-protos/src/** out/resources/protos --verbose",
    "nodemon": "nodemon --debug",
    "wait": "wait-on file:out/index.js",
    "serve": "npm-run-all wait nodemon",
    "start": "npm-run-all clean --parallel \"build:* -- --watch\" serve"
  },
  "author": "Luke Tillman <luke@luketillman.com> (http://www.luketillman.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukeTillman/killrvideo-web-server.git"
  },
  "files": [
    "out"
  ],
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "cpx": "^1.2.1",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.5.1",
    "rimraf": "^2.5.2",
    "wait-on": "^1.3.1"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "falcor-express": "^0.1.2",
    "falcor-json-graph": "^1.1.7",
    "falcor-router": "^0.3.0",
    "grpc": "^0.13.1",
    "killrvideo-web": "https://github.com/LukeTillman/killrvideo-web/releases/download/v1.0.0-beta.1/killrvideo-web-1.0.0-beta.1.tgz",
    "lodash": "^4.5.1",
    "moment": "^2.11.2",
    "socket.io": "^1.4.5",
    "uuid": "^2.0.1",
    "vhost": "^3.0.2"
  }
}
